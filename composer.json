{
    "name": "benmag/laravel-rancher",
    "version": "1.0.2",
    "description": "Rancher API wrapper for Laravel",
    "keywords": ["benmag","rancher", "docker"],
    "homepage": "https://github.com/benmag/laravel-rancher",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Mag",
            "homepage": "http://github.com/benmag"
        }
    ],
    "require": {
        "illuminate/support": "~5.5",
        "php" : "~5.5|~7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Benmag\\Rancher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benmag\\Rancher\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
